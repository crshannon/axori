{
  "name": "@axori/db",
  "version": "0.1.0",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./types": "./src/types-only.ts",
    "./src/schema": "./src/schema/index.ts",
    "./src/schema/*": "./src/schema/*"
  },
  "scripts": {
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/migrations/runner.ts --apply",
    "db:status": "tsx src/migrations/runner.ts --status",
    "db:verify": "tsx src/migrations/runner.ts --verify",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/seed/markets.ts",
    "db:seed:user": "tsx src/seed/mock-user-data.ts",
    "db:cleanup:user": "tsx src/seed/cleanup-user-data.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "drizzle-orm": "0.45.1",
    "postgres": "^3.4.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "drizzle-kit": "^0.30.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
