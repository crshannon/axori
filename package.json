{
  "name": "axori",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter @axori/web dev",
    "dev:web": "pnpm --filter @axori/web dev",
    "dev:api": "pnpm --filter @axori/api dev",
    "dev:mobile": "pnpm --filter @axori/mobile dev",
    "build": "turbo run build",
    "build:web": "pnpm --filter @axori/web build",
    "build:api": "pnpm --filter @axori/api build",
    "build:mobile": "pnpm --filter @axori/mobile build",
    "start:api": "pnpm --filter @axori/api start",
    "preview:web": "pnpm --filter @axori/web preview",
    "test": "turbo run test",
    "test:e2e": "pnpm --filter @axori/web test:e2e",
    "test:e2e:ui": "pnpm --filter @axori/web test:e2e:ui",
    "test:e2e:debug": "pnpm --filter @axori/web test:e2e:debug",
    "lint": "turbo run lint",
    "lint:web": "pnpm --filter @axori/web lint",
    "lint:api": "pnpm --filter @axori/api lint",
    "lint:mobile": "pnpm --filter @axori/mobile lint",
    "format": "pnpm --filter @axori/web format",
    "check": "pnpm --filter @axori/web check",
    "type-check": "turbo run type-check",
    "type-check:web": "pnpm --filter @axori/web type-check",
    "type-check:api": "pnpm --filter @axori/api type-check",
    "type-check:mobile": "pnpm --filter @axori/mobile type-check",
    "db:generate": "pnpm --filter @axori/db db:generate",
    "db:migrate": "pnpm --filter @axori/db db:migrate",
    "db:status": "pnpm --filter @axori/db db:status",
    "db:verify": "pnpm --filter @axori/db db:verify",
    "db:push": "pnpm --filter @axori/db db:push",
    "db:pull": "pnpm --filter @axori/db db:pull",
    "db:studio": "pnpm --filter @axori/db db:studio",
    "db:seed": "pnpm --filter @axori/db db:seed",
    "db:seed:user": "pnpm --filter @axori/db db:seed:user",
    "db:seed:cleanup": "pnpm --filter @axori/db db:cleanup:user",
    "clean": "turbo run clean"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "pnpm": {
    "overrides": {
      "drizzle-orm": "0.45.1",
      "@types/pg": "8.16.0"
    }
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "drizzle-zod": "^0.8.3"
  }
}
